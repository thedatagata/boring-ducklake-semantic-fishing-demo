# Example secrets configuration for filter_data_swamp pipeline
# Copy this file to secrets.toml and fill in your actual credentials
# NEVER commit secrets.toml to git!

[sources.filesystem.credentials]
project_id = "your-gcp-project-id"
private_key = "-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"
client_email = "your-service-account@your-project.iam.gserviceaccount.com"

[destination.motherduck.credentials]
# Get your token from https://app.motherduck.com/
database = "analytics"
password = "your_motherduck_token_here"
